{
  "name": "js-app-boilerplate",
  "version": "1.5.1",
  "description": "",
  "scripts": {
    "dev": "nodemon src/index.js --exec babel-node",
    "start": "node lib/index.js",
    "build": "babel src --out-dir lib --copy-files",
    "test": "jest --onlyChanged --watch",
    "test:all": "jest",
    "format": "prettier --single-quote --trailing-comma all '{src,__{tests,mocks}__}/**/*.{js,jsx}' --write",
    "precommit": "lint-staged",
    "clean": "rimraf lib"
  },
  "author": "bob61016@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/env-setup.js",
      "<rootDir>/node_modules/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/env-setup.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn format",
      "git add"
    ]
  }
}
